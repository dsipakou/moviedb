<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
	<head>
		<title></title>
		<%= javascript_include_tag "application" %>
		<%= javascript_include_tag "http://code.jquery.com/ui/1.10.1/jquery-ui.js" %>
		<%= javascript_include_tag "alertify.js" %>
		<%= javascript_include_tag "draggable-0.1.js" %>
		<%= javascript_include_tag "jquery.slider.js" %>
		<%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=PT+Sans&subset=latin,cyrillic" %>
		<%= stylesheet_link_tag 'default.css' %>
		<%= stylesheet_link_tag "jquery-ui.css" %>
		<%= stylesheet_link_tag "alertify.default.css" %>
		<%= stylesheet_link_tag "alertify.core.css" %>
		<%= stylesheet_link_tag "jslider.css" %>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	</head>
	<body>
		<div id="container">
			<div id="header">
				<%= render :partial => "navi" %>
			</div>
		</div>
		<%= yield %>
	</body>
</html>